{
  "name": "xuder",
  "version": "0.0.1",
  "description": "Build on the simplicity of Redux. A predicable state container.",
  "main": "index.js",
  "scripts": {
    "lint": "standard --fix",
    "test": "ava --tap",
    "watch": "ava --tap -watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenzhihao/Xuder.git"
  },
  "keywords": [
    "redux",
    "state"
  ],
  "author": "cabtravis@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chenzhihao/Xuder/issues"
  },
  "homepage": "https://github.com/chenzhihao/Xuder#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "standard": "^10.0.2"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
